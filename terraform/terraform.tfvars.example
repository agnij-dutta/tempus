# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your values

# AWS Region
region = "ap-south-1"

# Project name (used for resource naming)
project_name = "tempus"

# Environment name
environment = "dev"

# AWS Account ID (optional, will be fetched automatically if not provided)
# aws_account_id = "123456789012"

# Container image URL (from ECR)
# This should be set after building and pushing the Docker image
# Example: container_image = "123456789012.dkr.ecr.ap-south-1.amazonaws.com/tempus-backend:latest"
container_image = ""

# VPC Configuration (optional - uses default VPC if not specified)
# vpc_id = "vpc-12345678"
# subnet_ids = ["subnet-12345678", "subnet-87654321"]

